{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": [2],
    "linebreak-style": [2, "unix"],
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off",
    "no-multiple-empty-lines": [1, {"max": 1 }]
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
