{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": [2],
    "linebreak-style": [2, "unix"],
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off",
    "no-multiple-empty-lines": [1, {"max": 1 }],
    "semi": "off"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
